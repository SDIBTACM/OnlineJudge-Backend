spring:
  redis:
    port: 6379
    host: 192.168.246.128
    max-total: 4
    max-idle: 4
    min-idle: 4
    max-wait-millis: 1000
  mail:
    host:
    port:
    username:
    password:

  datasource:
    url: jdbc:mysql://127.0.0.1:3306/onlinejudge?useUnicode=true&characterEncoding=UTF-8
    username: root
    password:
    type: com.alibaba.druid.pool.DruidDataSource
    driver-class-name: com.mysql.cj.jdbc.Driver
    druid:
      filter:
        stat:
          enabled: true
          db-type: mysql
          log-slow-sql: true
          slow-sql-millis: 2000

  jpa:
    database: mysql
    show-sql: false
    hibernate:
      ddl-auto: update
    database-platform: org.hibernate.dialect.MySQL5InnoDBDialect

  http:
    encoding:
      charset: UTF-8

online-judge:
  # url generate will use it
  url: http://localhost/
  cache-store-type: REDIS
      mail:
        title: "Online Judge"
        sendBy: "ACM Group"
      debug:
        enable: false
        generator-data: false
        refresh-all-problem-solution-count: false